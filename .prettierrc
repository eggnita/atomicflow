{
    "semi": true,
    "tabWidth": 2,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "jsxSingleQuote": true,
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "useTabs": false,
    "bracketSameLine": false,
    "singleAttributePerLine": false
}